<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIconFile</key>
	<string></string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>$(PRODUCT_NAME)</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>1.0</string>
	<key>CFBundleVersion</key>
	<string>1</string>
	<key>LSMinimumSystemVersion</key>
	<string>11.0</string>
	<key>LSApplicationCategoryType</key>
	<string>public.app-category.productivity</string>
	<key>NSMainNibFile</key>
	<string>MainMenu</string>
	<key>NSPrincipalClass</key>
	<string>NSApplication</string>
	<key>LSUIElement</key>
	<true/>
	<key>NSSupportsAutomaticTermination</key>
	<true/>
	<key>NSSupportsSuddenTermination</key>
	<false/>
	<key>NSHumanReadableCopyright</key>
	<string>Copyright © 2024 vovo. All rights reserved.</string>
	
	<!-- 权限描述 -->
	<key>NSMicrophoneUsageDescription</key>
	<string>vovo 需要访问麦克风来录制语音并转换为文字。</string>
	<key>NSAppleEventsUsageDescription</key>
	<string>vovo 需要发送 Apple Events 来自动插入转换后的文字到其他应用程序中。</string>
	<key>NSSystemAdministrationUsageDescription</key>
	<string>vovo 需要系统管理权限来监听全局快捷键和插入文字。</string>
	
	<!-- 网络配置 -->
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<false/>
		<key>NSExceptionDomains</key>
		<dict>
			<key>volcengineapi.com</key>
			<dict>
				<key>NSExceptionAllowsInsecureHTTPLoads</key>
				<false/>
				<key>NSExceptionMinimumTLSVersion</key>
				<string>TLSv1.2</string>
				<key>NSIncludesSubdomains</key>
				<true/>
			</dict>
			<key>openspeech.bytedance.com</key>
			<dict>
				<key>NSExceptionAllowsInsecureHTTPLoads</key>
				<false/>
				<key>NSExceptionMinimumTLSVersion</key>
				<string>TLSv1.2</string>
				<key>NSIncludesSubdomains</key>
				<true/>
			</dict>
		</dict>
	</dict>
	
	<!-- 应用程序特性 -->
	<key>LSBackgroundOnly</key>
	<false/>
	<key>LSUIElement</key>
	<true/>
	<key>NSRequiresAquaSystemAppearance</key>
	<false/>
	
	<!-- 文档类型支持 -->
	<key>CFBundleDocumentTypes</key>
	<array>
		<dict>
			<key>CFBundleTypeExtensions</key>
			<array>
				<string>json</string>
			</array>
			<key>CFBundleTypeIconFile</key>
			<string></string>
			<key>CFBundleTypeName</key>
			<string>vovo Configuration</string>
			<key>CFBundleTypeRole</key>
			<string>Editor</string>
			<key>LSHandlerRank</key>
			<string>Owner</string>
			<key>LSItemContentTypes</key>
			<array>
				<string>public.json</string>
			</array>
		</dict>
	</array>
	
	<!-- URL Schemes -->
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleURLName</key>
			<string>vovo Configuration</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>vovo</string>
			</array>
		</dict>
	</array>
	
	<!-- 导出的类型标识符 -->
	<key>UTExportedTypeDeclarations</key>
	<array>
		<dict>
			<key>UTTypeConformsTo</key>
			<array>
				<string>public.json</string>
			</array>
			<key>UTTypeDescription</key>
			<string>vovo Configuration File</string>
			<key>UTTypeIdentifier</key>
			<string>com.vovo.recordtotext.config</string>
			<key>UTTypeTagSpecification</key>
			<dict>
				<key>public.filename-extension</key>
				<array>
					<string>vovo-config</string>
				</array>
				<key>public.mime-type</key>
				<array>
					<string>application/json</string>
				</array>
			</dict>
		</dict>
	</array>
	
	<!-- 服务提供 -->
	<key>NSServices</key>
	<array>
		<dict>
			<key>NSMenuItem</key>
			<dict>
				<key>default</key>
				<string>使用 vovo 转换语音</string>
			</dict>
			<key>NSMessage</key>
			<string>convertSpeechToText</string>
			<key>NSPortName</key>
			<string>vovo</string>
			<key>NSReturnTypes</key>
			<array>
				<string>NSStringPboardType</string>
			</array>
			<key>NSSendTypes</key>
			<array>
				<string>NSStringPboardType</string>
			</array>
		</dict>
	</array>
	
	<!-- 应用程序支持的语言 -->
	<key>CFBundleLocalizations</key>
	<array>
		<string>en</string>
		<string>zh-Hans</string>
		<string>zh-Hant</string>
	</array>
	
	<!-- 应用程序信息 -->
	<key>CFBundleDisplayName</key>
	<string>vovo</string>
	<key>CFBundleGetInfoString</key>
	<string>vovo 1.0, Copyright © 2024 vovo. All rights reserved.</string>
	<key>NSContactsUsageDescription</key>
	<string>vovo 不会访问您的通讯录。</string>
	<key>NSCalendarsUsageDescription</key>
	<string>vovo 不会访问您的日历。</string>
	<key>NSRemindersUsageDescription</key>
	<string>vovo 不会访问您的提醒事项。</string>
	
	<!-- 硬件要求 -->
	<key>UIRequiredDeviceCapabilities</key>
	<array>
		<string>microphone</string>
	</array>
	
	<!-- 应用程序类别和关键词 -->
	<key>LSApplicationCategoryType</key>
	<string>public.app-category.productivity</string>
	<key>ITSAppUsesNonExemptEncryption</key>
	<false/>
	
	<!-- 启动配置 -->
	<key>NSHighResolutionCapable</key>
	<true/>
	<key>NSSupportsAutomaticTermination</key>
	<true/>
	<key>NSSupportsSuddenTermination</key>
	<false/>
	
	<!-- 调试和开发配置 -->
	<key>NSAppleScriptEnabled</key>
	<true/>
	<key>OSAScriptingDefinition</key>
	<string>vovo.sdef</string>
</dict>
</plist>